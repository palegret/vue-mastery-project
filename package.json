{
  "name": "vue-mastery-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "start": "concurrently \"vue-cli-service serve\" \"json-server --watch src/data/db.json\"",
    "start-slow": "concurrently \"vue-cli-service serve\" \"json-server --watch --delay 1500 src/data/db.json\"",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "0.19.0",
    "json-server": "0.15.1",
    "lodash": "4.17.15",
    "nprogress": "^0.2.0",
    "vue": "^2.5.22",
    "vue-router": "3.1.3",
    "vuejs-datepicker": "1.6.2",
    "vuetify": "2.0.11",
    "vuex": "3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.6.0",
    "@vue/cli-plugin-eslint": "3.6.0",
    "@vue/cli-service": "3.6.0",
    "babel-eslint": "^10.0.1",
    "concurrently": "^5.0.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "keywords": [],
  "description": "For courses 'Real World Vue' and 'Mastering Vuex'."
}
